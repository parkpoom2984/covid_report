{
  "name": "covid_report",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-* && watchman watch-del-all && npm cache clean --force && npm cache verify",
    "bundle-android": "yarn bundle-option -- --platform android --bundle-output bundle/index.android.bundle.js",
    "bundle-option": "react-native bundle --dev false --entry-file index.js",
    "bundle-android-apply": "shx cp -R bundle/android .",
    "android-release": "yarn bundle-android && yarn bundle-android-apply && yarn android-build",
    "android:dev": "ENVFILE=./src/config/env/.env.dev && yarn android --variant devDebug --appIdSuffix dev",
    "android:qa": "ENVFILE=./src/config/env/.env.qa && yarn android --variant qaDebug --appId=com.covid_report.qa",
    "android:prod": "ENVFILE=./src/config/env/.env.prod && yarn android --variant prodDebug",
    "android-build": "cd android && ./gradlew assembleRelease --stacktrace"
  },
  "dependencies": {
    "axios": "0.21.1",
    "lodash": "4.17.21",
    "mobx": "5.15.1",
    "mobx-react": "6.1.4",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-config": "^1.4.3",
    "react-native-flags": "1.0.0",
    "react-native-gesture-handler": "1.9.0",
    "react-native-orientation": "https://github.com/tomyo-edtech/react-native-orientation#master",
    "react-native-reanimated": "1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "2.17.1",
    "react-native-splash-screen": "3.2.0",
    "react-native-walkthrough-tooltip": "^1.2.0",
    "react-navigation": "4.4.3",
    "react-navigation-stack": "^2.10.2",
    "rn-placeholder": "3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-test-renderer": "^16.9.2",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-mobx": "^2.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-wix": "1.1.21",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-typescript": "0.13.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "shx": "^0.3.3",
    "typescript": "^3.8.3",
    "typescript-eslint-parser": "22.0.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
